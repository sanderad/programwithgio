{
    "require": {
        "ramsey/uuid": "^4.7",
        "vlucas/phpdotenv": "^5.5",
        "psr/container": "^2.0",
        "symfony/mailer": "^6.3",
        "doctrine/dbal": "^3.7",
        "doctrine/orm": "^2.16",
        "symfony/cache": "^6.3",
        "doctrine/migrations": "^3.6",
        "illuminate/database": "^10.30",
        "illuminate/events": "^10.30",
        "guzzlehttp/guzzle": "7.4",
        "twig/twig": "^3.7",
        "twig/intl-extra": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    }
}
